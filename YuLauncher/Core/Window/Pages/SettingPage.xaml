<Page x:Class="YuLauncher.Core.Pages.SettingPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:properties="clr-namespace:YuLauncher.Properties"
             xmlns:local="clr-namespace:YuLauncher.Core.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="{Binding Source={x:Static properties:Settings.Default},Path=Window_H}" d:DesignWidth="{Binding Source={x:Static properties:Settings.Default},Path=Window_W}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <StackPanel
                    Orientation="Horizontal"
                    ZIndex="4" Grid.RowSpan="2" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <Label Content="フルスクリーンON/OFF" Height="30" Width="120" Margin="0,-300,0,0"/>

        </StackPanel>
    </Grid>
</Page>
